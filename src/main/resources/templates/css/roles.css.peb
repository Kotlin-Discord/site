{# @pebvariable name="roles" type="List<com.kotlindiscord.api.client.models.RoleModel>" #}
{% for role in roles %}
.background-{{ role.name }} {background-color: #{{ role.colour }} !important;}
.border-{{ role.name }} {border-color: #{{ role.colour }} !important;}
.color-{{ role.name }} {color: #{{ role.colour }} !important;}

{% endfor %}
