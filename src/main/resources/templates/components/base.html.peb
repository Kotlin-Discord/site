<!DOCTYPE html>
<html lang="en">
<head>
    <title>Kotlin Discord | {% block title %}No Title{% endblock %}</title>

    {# Below block generated by realfavicongenerator.net #}
    <link rel="apple-touch-icon" sizes="180x180" href="/static/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/static/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="194x194" href="/static/icons/favicon-194x194.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/static/icons/android-chrome-192x192.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/static/icons/favicon-16x16.png">
    <link rel="manifest" href="/static/icons/site.webmanifest">
    <link rel="mask-icon" href="/static/icons/safari-pinned-tab.svg" color="#7289da">
    <link rel="shortcut icon" href="/static/icons/favicon.ico">
    <meta name="apple-mobile-web-app-title" content="Kotlin Discord">
    <meta name="application-name" content="Kotlin Discord">
    <meta name="msapplication-TileColor" content="#7289da">
    <meta name="msapplication-TileImage" content="/static/icons/mstile-144x144.png">
    <meta name="msapplication-config" content="/static/icons/browserconfig.xml">
    <meta name="theme-color" content="#7289da">
    {# End generated block #}

    {# OpenGraph stuff #}
    <meta property="og:title" content="Kotlin Discord | {% block title %}No Title{% endblock %}" />
    <meta property="og:description" content="{% block description %}A friendly community for Kotlin beginners and advanced users alike.{% endblock %}" />
    <meta property="og:image" content="{% block embedImage %}/static/images/logo-wide.png{% endblock %}" />
    {# End OpenGraph stuff #}

    {# Twitter card stuff #}
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:title" content="Kotlin Discord | {% block title %}No Title{% endblock %}" />
    <meta name="twitter:description" content="{% block description %}A friendly community for Kotlin beginners and advanced users alike.{% endblock %}" />
    <meta name="twitter:image" content="{% block embedImage %}/static/images/logo-wide.png{% endblock %}" />
    {# End Twitter card stuff #}

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="/static/semantic.css"/>
    <link rel="stylesheet" href="/static/css/main.css"/>
    <link rel="stylesheet" href="/generated/roles.css"/>
    <script type="text/javascript" src="/static/jquery.js"></script>
    <script type="text/javascript" src="/static/semantic.js"></script>
</head>
<body>
{% block body %}

{% include "components/nav/mobile.html.peb" %}

<div class="pusher">
    <div class="flex-columns">
        <div class="full height nav column">
        {% include "components/nav/desktop.html.peb" %}
        </div>

        <div class="content column" id="content-pusher">
        {% include "components/nav/mobile.inner.html.peb" %}

            <div class="ui content">
            {% block content %}{% endblock %}
            </div>
        </div>
    </div>
</div>

{% endblock %}

<script type="text/javascript" src="/static/js/main.js"></script>
</body>
</html>
